{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ],
    "outputDirectory": ".",
    "buildCommand": "cd edtech/backend && npm install"
} 